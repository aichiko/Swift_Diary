source 'https://github.com/CocoaPods/Specs.git'

target “Swift_Diary” do
use_frameworks!

platform :ios,'10.0' #Alamofire 4.0 以上必须要改成iOS 10.0 Alamofire 3.x 并不支持swift3.0
pod 'ReactiveCocoa', :git => 'https://github.com/ReactiveCocoa/ReactiveCocoa.git'

pod 'Alamofire', '~> 4.0'

pod 'SnapKit', '~> 3.0.2'

end
